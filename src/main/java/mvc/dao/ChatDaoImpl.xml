<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="mvc.dao.ChatDao">
  
	  <insert id="insertChat" parameterType="mvc.dto.ChatDto">
	  		insert into chat(chat_no,chat_content,chatroom_id,uesr_no) values(chat_no = chat_seq.nextval,chat_content = #{chat_content},chatroom_id = #{chatroom_id},uesr_no = #{uesr_no});
	  </insert>
  
  </mapper>